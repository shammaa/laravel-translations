{
    "name": "shammaa/laravel-translations",
    "description": "Professional, high-performance multilingual translation system for Laravel with optimized database views and advanced features",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Shadi Shammaa",
            "email": "shadi.shammaa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\LaravelTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shammaa\\LaravelTranslations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\LaravelTranslations\\LaravelTranslationsServiceProvider"
            ],
            "aliases": {
                "Translations": "Shammaa\\LaravelTranslations\\Facades\\Translations"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

